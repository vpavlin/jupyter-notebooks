{
    "kind": "List",
    "apiVersion": "v1",
    "items": [
        {
            "kind": "ImageStream",
            "apiVersion": "v1",
            "metadata": {
                "name": "s2i-minimal-notebook",
                "labels": {
                    "build": "s2i-minimal-notebook"
                }
            },
            "spec": {
                "lookupPolicy": {
                    "local": true
                }
            }
        },
        {
            "kind": "BuildConfig",
            "apiVersion": "v1",
            "metadata": {
                "name": "s2i-minimal-notebook",
                "labels": {
                    "build": "s2i-minimal-notebook"
                }
            },
            "spec": {
                "triggers": [
                    {
                        "type": "ConfigChange"
                    },
                    {
                        "type": "ImageChange"
                    }
                ],
                "source": {
                    "type": "Git",
                    "git": {
                        "uri": "https://github.com/vpavlin/jupyter-notebooks"
                    },
                    "contextDir": "minimal-notebook"
                },
                "strategy": {
                    "type": "Source",
                    "sourceStrategy": {
                        "from": {
                            "kind": "DockerImage",
                            "name": "registry.access.redhat.com/ubi7/python-36"
                        }
                    }
                },
                "output": {
                    "to": {
                        "kind": "ImageStreamTag",
                        "name": "s2i-minimal-notebook:3.6"
                    }
                },
                "resources": {
                    "limits": {
                        "memory": "1Gi"
                    }
                }
            }
        },
        {
            "kind": "ImageStream",
            "apiVersion": "v1",
